<!-- prettier-ignore -->
<%_
_%>

<!-- 電話番号 -->
<!-- wordPress化する時はinputタグをCONTACT7用に置換える -->
<div class="p-contact__form--item">
  <div class="p-contact__form--title">
    <label for="tel" class="p-contact__form--title-text">電話番号</label>
    <span class="p-contact__form--require">必須</span>
  </div>
  <div class="p-contact__form--input">
    <span class="wpcf7-form-control-wrap" data-name="tel"
      ><input
        size="40"
        class="wpcf7-form-control wpcf7-tel wpcf7-validates-as-required wpcf7-text wpcf7-validates-as-tel reserve__form--input js-tel-number"
        id="tel"
        aria-required="true"
        aria-invalid="false"
        placeholder="000-0000-0000"
        value=""
        type="tel"
        name="tel"
        required="required"
    /></span>
    <div class="error-message"></div>
  </div>
</div>
